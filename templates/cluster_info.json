{
    "name" : "pbscluster",
    "worker_bin_path" : "/tmp/cryosparcw",
    "cache_path" : "/tmp/cryosparc_cache",
    "send_cmd_tpl" : "{{ command }}",
    "qsub_cmd_tpl" : "qsub.py --num-cpu={{ num_cpu }} --num-gpu={{ num_gpu }} --run-cmd={{ script_path_abs }} --namespace=zdeneklapeseinfracesnetcz-ns",
    "qstat_cmd_tpl" : "qstat.py --job-name={{ cluster_job_id }} --namespace=zdeneklapeseinfracesnetcz-ns",
    "qdel_cmd_tpl" : "qdel.py --job-name={{ cluster_job_id }} --namespace=zdeneklapeseinfracesnetcz-ns",
    "qinfo_cmd_tpl" : "qstat.py --job-name={{ cluster_job_id }}",
    "transfer_cmd_tpl" : "{{ src_path }} {{ dest_path }}"
}
